{
  "name": "react-lowdb-express-boilerplate",
  "version": "1.0.0",
  "description": "React with webpack (hot module replacement enabled) and Bootstrap",
  "main": "app.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server -d --hot --config ./config/webpack.dev.js",
    "build": "NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "wdev": "SET NODE_ENV=development & webpack-dev-server -d --hot --config ./config/webpack.dev.js",
    "wbuild": "SET NODE_ENV=production & webpack --config ./config/webpack.prod.js",
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinothpandian/react-webpack-boilerplate.git"
  },
  "keywords": [
    "reactjs",
    "webpack",
    "react",
    "router",
    "hot",
    "module",
    "replacement",
    "lowDB",
    "express"
  ],
  "author": "Vinoth Pandian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinothpandian/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/vinothpandian/react-webpack-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.0.1",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^7.0.0",
    "nodemon": "^1.12.1",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "jquery": "^3.2.1",
    "lowdb": "^1.0.0",
    "octicons": "^7.0.1",
    "popper.js": "^1.12.8",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2"
  }
}
